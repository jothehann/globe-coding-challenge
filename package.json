	"devDependencies": {
		"@faker-js/faker": "^9.8.0",
		"@playwright/test": "^1.53.0",
		"@types/node": "^24.0.0",
		"@typescript-eslint/eslint-plugin": "^8.34.0",
		"@typescript-eslint/parser": "^8.34.0",
		"dotenv": "^16.5.0",
		"eslint": "^9.29.0",
		"eslint-config-prettier": "^10.1.5",
